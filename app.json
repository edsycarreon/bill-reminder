{
  "expo": {
    "name": "Bill Reminder",
    "slug": "bill-reminder-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bill-reminder.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.edsybitsy.billreminder"
    },
    "android": {
      "package": "com.edsybitsy.billreminder",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bill-reminder.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bill-reminder.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a19ec6be-c983-49f4-8f92-5529276a5e22"
      }
    }
  }
}
